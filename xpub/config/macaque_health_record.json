{
    "description": "prompts to present when creating a new trial", 
    "author": [ 
        "M. Best"
    ], 
    "updated_at": "2015-08-13", 
    "prompts": [
        {
            "info": "What is the study this health record is to be associated with?", 
            "regex": "\\w{3}", 
            "text": "Study for which this is a health record?", 
            "require": true, 
            "options": [
                "pig-chewing-study", 
                "cow-chewing-study", 
                "test study"
            ], 
            "key": "study", 
            "type": "list", 
            "example": "pig-chewing-study", 
            "store": [
                "hatabase"
            ]
        }, 
        {
            "info": "Specify the date of this health record: YYYY-MM-DD.", 
            "regex": "", 
            "text": "Specify the date of this health record: YYYY-MM-DD.", 
            "require": true, 
            "options": [], 
            "key": "date", 
            "type": "date", 
            "example": "2015-04-28", 
            "store": [
                "hatabase"
            ]
        }, 
        {
            "info": "Specify the name of the individual research subject.", 
            "regex": "\\w{3}", 
            "text": "Name of research subject?", 
            "require": true, 
            "options": [
                "Kiki", 
                "none"
            ], 
            "key": "subject_name", 
            "type": "list", 
            "example": "maxwell", 
            "store": [
                "hatabase"
            ]
        }, 
        {
            "info": "Specify the name of the primary trainer this session.", 
            "regex": "\\w{3}", 
            "text": "Name of experimenter?", 
            "require": true, 
            "options": [
                "Kazutaka Takahashi", 
                "none"
            ], 
            "key": "trainerA", 
            "type": "list", 
            "example": "maxwell", 
            "store": [
                "hatabase"
            ]
        },  
        {
            "info": "Specify the weight of the research subject in kilograms.", 
            "regex": "", 
            "text": "Weight of research subject in kilograms?", 
            "require": true, 
            "options": [], 
            "key": "subject_grams", 
            "type": "number", 
            "example": "10.0", 
            "store": [
                "hatabase"
            ]
        }, 
        {
            "info": "Specify the amount of fluid the animal received from the juicer (mL).", 
            "regex": "", 
            "text": "How much fluid did the juicer supply (mL)?", 
            "require": true, 
            "options": [], 
            "key": "drinker_supp", 
            "type": "number", 
            "example": "100.0", 
            "store": [
                "hatabase"
            ]
        },
        {
            "info": "Specify the amount of fluid that the animal was supplemented (mL).", 
            "regex": "", 
            "text": "How much fluid did the trainer supplement (mL)?", 
            "require": true, 
            "options": [], 
            "key": "drinker_supp", 
            "type": "number", 
            "example": "100.0", 
            "store": [
                "hatabase"
            ]
        },  
        {
            "info": "Specify the time that the animal got in the chair.", 
            "regex": "^(0[0-9]|1[0-9]|2[0-3]):[0-5][0-9]$", 
            "text": "When was the animal chaired (HH:MM)?",
            "require": true, 
            "options": [], 
            "key": "chair_start", 
            "type": "text", 
            "example": "01:00", 
            "store": [
                "hatabase"
            ]
        },
        {
            "info": "Specify the time that the animal began playing its game.", 
            "regex": "^(0[0-9]|1[0-9]|2[0-3]):[0-5][0-9]$", 
            "text": "When did the animal start playing (HH:MM)?",
            "require": true, 
            "options": [], 
            "key": "play_start", 
            "type": "text", 
            "example": "01:00", 
            "store": [
                "hatabase"
            ]
        },
        {
            "info": "Specify the time that the animal stopped playing its game.", 
            "regex": "^(0[0-9]|1[0-9]|2[0-3]):[0-5][0-9]$", 
            "text": "When did the animal stop playing (HH:MM)?",
            "require": true, 
            "options": [], 
            "key": "play_end", 
            "type": "text", 
            "example": "01:00", 
            "store": [
                "hatabase"
            ]
        },
        {
            "info": "Specify the time that the animal returned to its cage.", 
            "regex": "^(0[0-9]|1[0-9]|2[0-3]):[0-5][0-9]$", 
            "text": "When did the animal return to its cage (HH:MM)?",
            "require": true, 
            "options": [], 
            "key": "chair_end", 
            "type": "text", 
            "example": "01:00", 
            "store": [
                "hatabase"
            ]
        }, 
        {
            "info": "Was the animal bright, alert, and responsive?", 
            "regex": "", 
            "text": "Was the animal BAR?", 
            "require": true, 
            "options": ["BAR"], 
            "key": "psych", 
            "type": "list", 
            "example": "", 
            "store": [
                "hatabase"
            ]
        },
        {
            "info": "Was the animal's skin elastic?", 
            "regex": "", 
            "text": "Was the animal's skin elastic?", 
            "require": true, 
            "options": ["elastic"], 
            "key": "skin", 
            "type": "list", 
            "example": "", 
            "store": [
                "hatabase"
            ]
        },
        {
            "info": "What was the status of the animal's gums?", 
            "regex": "", 
            "text": "What was the status of the animal's gums?", 
            "require": true, 
            "options": ["pink, moist"], 
            "key": "gums", 
            "type": "list", 
            "example": "", 
            "store": [
                "hatabase"
            ]
        },
        {
            "info": "Was urine observed?", 
            "regex": "", 
            "text": "Was urine observed?", 
            "require": true, 
            "options": ["none observed", "present"], 
            "key": "urine", 
            "type": "list", 
            "example": "", 
            "store": [
                "hatabase"
            ]
        },
        {
            "info": "Was feces observed?", 
            "regex": "", 
            "text": "Was feces observed?", 
            "require": true, 
            "options": ["none observed", "present"], 
            "key": "feces", 
            "type": "list", 
            "example": "", 
            "store": [
                "hatabase"
            ]
        },
        {
            "info": "Was the animal's head cleaned?", 
            "regex": "", 
            "text": "Was the animal's head cleaned?", 
            "require": true, 
            "options": ["head clean"], 
            "key": "headclean", 
            "type": "list", 
            "example": "", 
            "store": [
                "hatabase"
            ]
        },

        {
            "info": "Provide any additional comments.", 
            "regex": "\\w{3}", 
            "text": "Anything else to note?", 
            "require": false, 
            "options": [], 
            "key": "healthcomments", 
            "type": "text", 
            "example": "", 
            "store": [
                "hatabase"
            ]
        }
    ], 
    "version": "1.0.0", 
    "key": "macaque_health_record"
}
